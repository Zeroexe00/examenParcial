@model examenParcial.Models.Vaca
@{
    ViewBag.Title = "FichaBovina";
}

<h2>FichaBovina</h2>



<fieldset class="form-horizontal">
    <legend class="label"> Informacion de Animal</legend>
    <div class="form-group">
        <div class=""> DIIO</div>
        <div class="text-muted"> @Html.DisplayFor(m => m.diio) </div>
    </div>
    <div class="form-group">
        <div class=""> Nombre</div>
        <div class="text-muted"> @Html.DisplayFor(m => m.nombre) </div>
    </div>
    <div class="form-group">
        <div class=""> Sexo</div>
        <div class="text-muted"> @Html.DisplayFor(m => m.sexo) </div>
    </div>
    <div class="form-group">
        <div class=""> raza</div>
        <div class="text-muted"> @Html.DisplayFor(m => m.raza) </div>
    </div>
    <div class="form-group">
        <div class=""> Edad</div>
        <div class="text-muted"> @Html.DisplayFor(m => m.edad) </div>
    </div>
    <div class="form-group">
        <div class=""> Tipo de animal</div>
        <div class="text-muted"> @Html.DisplayFor(m => m.tipo) </div>
    </div>
    <div class="form-group">
        <div class=""> Rebaño</div>
        <div class="text-muted"> @Html.DisplayFor(m => m.rebaño) </div>
    </div>
</fieldset>
        